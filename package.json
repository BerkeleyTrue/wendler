{
  "name": "wendler",
  "version": "0.0.1",
  "description": "wendler app",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "lint": "eslint --ext=js,jsx .",
    "lint:fix": "eslint . --ext=js,jsx --fix",
    "format": "prettier --trailing-comma all --single-quote --write '*.js' 'src/**/*.!(global.sss|sss)' '!src/**/*.test.js.snap'",
    "postformat": "npm run lint:fix",
    "build": "npm run build:client && npm run build:server:client && npm run build:server",
    "build:client": "BABEL_TARGET=client webpack --env.production=true",
    "build:server": "NODE_ENV=production BABEL_TARGET=node babel src/server --ignore src/server/common-to-server.js --out-dir dist/server",
    "build:server:client": "BABEL_TARGET=node webpack --config webpack.server.js --env production",
    "pretest": "npm run lint",
    "test": "nyc jest ./src",
    "test:watch": "jest --watch",
    "cover": "nyc report --reporter=html",
    "cover:watch": "browser-sync start --ss coverage --port 3001 -f coverage/index.html & nodemon --watch src/**/* --watch test/**/* --delay 2 --exec 'npm run test && npm run cover'",
    "cover:alls": "nyc report --reporter=text-lcov | coveralls",
    "cover:open": "open coverage/index.html",
    "commit": "git-cz",
    "prepublishOnly": "npm run build",
    "webpack:client:dashboard": "webpack-dashboard -p 3005",
    "webpack:server:dashboard": "webpack-dashboard -p 3006"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "jest": {
    "bail": true
  },
  "nyc": {
    "extensions": [
      ".js",
      ".jsx"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "apollo-server-express": "^2.4.2",
    "arangojs": "^6.2.4",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "debug": "^4.0.0",
    "dedent": "^0.7.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-state": "^1.4.0",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4",
    "history": "^4.7.2",
    "invariant": "^2.2.1",
    "isdev": "^1.0.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "nodemailer": "^5.0.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.16.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-redux-form": "^1.16.8",
    "redux": "^4.0.0",
    "redux-first-router": "^1.9.19",
    "redux-first-router-link": "^1.4.2",
    "redux-observable": "^1.0.0",
    "redux-vertical": "^1.1.2",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "source-map-support": "^0.5.4",
    "sugarss": "^1.0.1",
    "uid2": "0.0.3",
    "validator": "^10.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-ramda": "^2.0.0",
    "babel-plugin-transform-imports": "^1.5.0",
    "bcrypt": "^3.0.4",
    "browser-sync": "^2.23.7",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.0.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^22.2.2",
