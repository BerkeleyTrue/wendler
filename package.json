{
  "name": "wendler",
  "version": "0.0.1",
  "description": "wendler app",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "lint": "eslint --ext=js,jsx .",
    "postlint": "npm run flow",
    "lint:fix": "eslint . --ext=js,jsx --fix",
    "flow": "flow check",
    "flow:cover": "flow-coverage-report -i 'src/**/*.js' -i 'src/**/*.jsx' -t html -o coverage",
    "format": "prettier '*.js' 'src/**/*.!(global.sss|sss)' --trailing-comma all --single-quote --write",
    "postformat": "npm run lint:fix",
    "build": "npm run build:client && npm run build:server:client && npm run build:server",
    "build:client": "BABEL_TARGET=client webpack --env production",
    "build:server": "NODE_ENV=production BABEL_TARGET=node babel src/server --ignore src/server/common-to-server.js --out-dir dist/server",
    "build:server:client": "BABEL_TARGET=node webpack --config webpack.server.js --env production",
    "pretest": "npm run lint",
    "test": "nyc jest",
    "test:watch": "jest --watch",
    "cover": "nyc report --reporter=html",
    "cover:watch": "browser-sync start --ss coverage --port 3001 -f coverage/index.html & nodemon --watch src/**/* --watch test/**/* --delay 2 --exec 'npm run test && npm run cover'",
    "cover:alls": "nyc report --reporter=text-lcov | coveralls",
    "cover:open": "open coverage/index.html",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "prepublishOnly": "npm run build"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "bail": true
  },
  "nyc": {
    "extensions": [
      ".js",
      ".jsx"
    ]
  },
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "arangojs": "^6.2.4",
    "babel-runtime": "^6.11.6",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "debug": "^3.1.0",
    "dedent": "^0.7.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-state": "^1.4.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.0",
    "history": "^4.7.2",
    "invariant": "^2.2.1",
    "isdev": "^1.0.1",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "nodemailer-smtp-transport": "^2.7.4",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.8.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-redux-form": "^1.16.8",
    "redux": "^4.0.0",
    "redux-first-router": "^1.9.19",
    "redux-first-router-link": "^1.4.2",
    "redux-observable": "^0.19.0",
    "redux-vertical": "^1.1.2",
    "rxjs": "^6.2.0",
    "source-map-support": "^0.5.4",
    "sugarss": "^1.0.1",
    "uid2": "0.0.3",
    "validator": "^10.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.23.7",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.5.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "git+https://github.com/berkeleytrue/eslint-plugin-react.git#feat/rwr",
    "flow-bin": "^0.74.0",
