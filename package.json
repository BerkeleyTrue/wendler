{
  "name": "wendler",
  "version": "0.0.1",
  "description": "wendler app",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:watch & npm run start:server",
    "start:server": "nodemon -w dist/server dist/server",
    "lint": "eslint --ext=js,jsx .",
    "lint:fix": "eslint . --ext=js,jsx --fix",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack",
    "build:server": "BABEL_ENV=node babel src/server --out-dir dist/server",
    "build:watch": "npm run build:watch:server & npm run build:watch:client",
    "build:watch:server": "nodemon --watch src/server --exec 'npm run build:server'",
    "build:watch:client": "webpack -w",
    "pretest": "npm run lint",
    "test": "nyc ava",
    "test:watch": "ava --watch",
    "cover": "nyc report --reporter=html",
    "cover:watch": "browser-sync start --ss coverage --port 3001 -f coverage/index.html & nodemon --watch src/**/* --watch test/**/* --delay 2 --exec 'npm run test && npm run cover'",
    "cover:alls": "nyc report --reporter=text-lcov | coveralls",
    "cover:open": "open coverage/index.html",
    "format": "prettier '*.js' 'src/**/*' 'test/**/*' --trailing-comma all --single-quote --write",
    "postformat": "npm run lint:fix",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "prepublishOnly": "npm run build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-freecodecamp"
    }
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "failFast": true
  },
  "dependencies": {
    "babel-plugin-react-css-modules": "^3.3.3",
    "babel-runtime": "^6.11.6",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "invariant": "^2.2.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-observable": "^0.18.0",
    "redux-vertical": "^1.0.0",
    "sugarss": "^1.0.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "cz-freecodecamp": "^1.0.0",
    "eslint": "^4.5.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "git+https://github.com/berkeleytrue/eslint-plugin-react.git#feat/rwr",
    "husky": "^0.14.3",
    "nodemon": "^1.17.3",
    "nyc": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "postcss-modules": "^1.1.0",
    "prettier": "^1.5.2",
    "react-hot-loader": "^4.0.1",
    "sinon": "^5.0.0",
    "style-loader": "^0.20.3",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-middleware": "^3.1.0",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-stats-plugin": "^0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BerkeleyTrue/wendler.git"
  },
  "keywords": [
    "react",
    "redux",
    "wendler"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BerkeleyTrue/wendler/issues"
  },
  "homepage": "https://github.com/BerkeleyTrue/wendler#readme"
}
